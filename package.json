{
  "name": "@attrs/style",
  "version": "0.7.0",
  "description": "attrs css theme",
  "main": "src/index.js",
  "module": "src/index.js",
  "homepage": "https://github.com/attrs/style",
  "repository": "attrs/style",
  "author": "joje <joje.attrs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run dist",
    "start": "httpd serve docs -p 9200 -s",
    "dev": "webpack-dev-server --host 0.0.0.0 --port 9200 --config build/webpack.docs.config.js",
    "dist": "npm run format && npm run dist:docs && npm run dist:lib",
    "dist:lib": "rimraf lib && cross-env NODE_ENV=production webpack --config build/webpack.lib.config.js",
    "dist:docs": "rimraf docs && cross-env NODE_ENV=production webpack --config build/webpack.docs.config.js",
    "watch": "webpack --config build/webpack.lib.config.js --watch",
    "format": "prettier --write \"src/**/*.{js,css}\""
  },
  "dependencies": {
    "@attrs/httpd": "^0.1.0",
    "debounce": "^1.2.0"
  },
  "devDependencies": {
    "@attrs/router": "^0.6.3",
    "@babel/core": "^7.10.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "postcss-import-sync": "^7.1.4",
    "postcss-js": "^3.0.3",
    "prettier": "^2.0.5",
    "purgecss-webpack-plugin": "^2.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "worklet-loader": "^1.0.0"
  },
  "files": [
    "lib",
    "src",
    "build",
    ".browserslistrc",
    ".stylelintrc",
    "postcss.config.js",
    "tailwind.config.js",
    "plugin.js",
    "preset.js",
    "LICENSE",
    "README.md"
  ]
}